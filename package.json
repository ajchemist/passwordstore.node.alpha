{
  "name": "@magneto-labs/passwordstore.node.alpha",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "test": "jest",
    "clean": "rm -rf dist",
    "build:type": "tsc --noEmit false --declaration --emitDeclarationOnly --outDir dist/type",
    "build:esm": "tsc --noEmit false",
    "build:cjs": "tsc --noEmit false --module commonjs --moduleResolution node --outDir dist/cjs"
  },
  "files": [
    "dist/"
  ],
  "exports": {
    "./*": {
      "default": "./dist/cjs/*.js",
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js",
      "types": "./dist/type/*.d.ts"
    }
  },
  "version": "2023.349.27400"
}
